public Id insertCourse(String name, String details, Integer credits) {
    if(credits == null) 
    return null;
    apxio__Course__c cou = new apxio__Course__c();
    cou.Name = name;
    cou.apxio__Course_Details__c = details;
    cou.apxio__Credits__c = String.valueOf(credits);
    return cou.Id;
}