public Integer rockPaperScissors(String player1, String player2) {
    if(player1 == player2)
        return 0;
    Map<String,String> mapWinPlay = new Map <String,String>{
        'rock' => 'scissors',
        'scissors' => 'paper',
        'paper' => 'rock'
    };
    return mapWinPlay.get(player1).equals(player2) ? '1' : '2' ;
}