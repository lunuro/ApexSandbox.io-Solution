public Boolean isKeyAccount(Account a) {
    Map<String,Integer> mapKeyIndRev = new Map<String,Integer>{
        'Banking' => 600000,
        'Technology' => 800000,
        'Retail' => 2000000
     };
    if(a?.AnnualRevenue >= mapKeyIndRev.get(a.Industry)
        || (!mapKeyIndRev.containsKey(a?.Industry) && a?.AnnualRevenue >= 500000))
        return true;
    return false;
}