public String whichTwo(Integer a, Integer b, Integer c) {
    Integer absA = Math.abs(a);
    Integer absB = Math.abs(b);
    Integer absC = Math.abs(c);
    if(absA + absB == absC) 
        return 'c';
    if(absA + absC == absB)
        return 'b';
    if(absB + absA == absA)
        return 'a';
    return '';
}