public List<Account> getAccounts(List<Account> accounts) {
    accounts?.sort(new RevenueComparator());
    return accounts;
}

public class RevenueComparator implements Comparator<Account> {
    public Integer compare(Account acc1, Account acc2) {
        Integer returnValue = 0;
        if ((acc1?.AnnualRevenue == null) && (acc2?.AnnualRevenue == null)) {
            returnValue = 0;
        } else if (acc1?.AnnualRevenue == null){
            returnValue = -1;
        } else if (acc2?.AnnualRevenue == null){
            returnValue = 1;
        } else if (acc1.AnnualRevenue > acc2.AnnualRevenue) {
            returnValue = -1;
        } else if (acc1.AnnualRevenue < acc2.AnnualRevenue) {
            returnValue = 1;
        }
        return returnValue;
    }
}